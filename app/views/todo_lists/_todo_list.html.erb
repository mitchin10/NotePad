<% if todo_list.completed? %>
<div class="todo-list-item">
  <ul>
    <li><strike><%= link_to todo_list.content, complete_note_todo_list_path(@note, todo_list.id), method: :patch %></strike></li>
  </ul>
</div>
<div class="actions icon-trash">
  <%= link_to "delete", note_todo_list_path(@note, todo_list.id), method: :delete, data: { confirm: "Are you sure?" } %>
</div>
<% else %>
<div class="todo-list-item">
  <ul>
    <li><%= link_to todo_list.content, complete_note_todo_list_path(@note, todo_list.id), method: :patch %></li>
  </ul>
</div>
<div class="actions icon-trash">
  <%= link_to "delete", note_todo_list_path(@note, todo_list.id), method: :delete, data: { confirm: "Are you sure?" } %>
</div>
<% end %>
